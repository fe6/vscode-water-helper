{
	"w-button/type": {
		"description": "设置按钮类型",
    "options": [
      "info",
      "border",
      "dashed",
      "danger"
    ],
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-button/size": {
		"description": "设置大小",
    "options": [
      "small",
      "large",
      "不设置"
    ],
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-button/htmlType": {
		"description": "设置 button 原生的 type 值",
		"optionType": "String",
		"defaultValue": "button"
	},
	"w-button/stop": {
		"description": "按钮点击是否冒泡， 0.9.0 新增",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-button/loading": {
		"description": "设置载入状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-button/disabled": {
		"description": "设置禁用状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-button/ghost": {
		"description": "幽灵属性，使按钮背景透明",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-button/className": {
		"description": "自定义按钮最外层的 class",
		"optionType": "String | Object | Array",
		"defaultValue": "-"
	},
	"w-button/click": {
		"description": "点击动效之后触发的点击事件，返回 {event, key}",
		"optionType": "Function",
		"defaultValue": "({event, key}) => {}"
	}
}