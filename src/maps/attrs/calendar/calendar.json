{
	"w-calendar/value": {
		"description": "当前日历的日期",
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-calendar/showDate": {
		"description": "是否显示日期面板",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-calendar/showMonth": {
		"description": "是否显示月份面板",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-calendar/showYear": {
		"description": "是否显示年份面板",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-calendar/showAge": {
		"description": "是否显示世纪面板",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-calendar/type": {
		"description": "日历类型",
    "options": [
      "week",
      "month",
      "year",
      "age",
      "date"
    ],
		"optionType": "String",
		"defaultValue": "date"
	},
	"w-calendar/defaultPickerValue": {
		"description": "当 v-model( value ) 为空时，选择器打开时默认显示的时间",
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-calendar/firstDayOfWeek": {
		"description": "周起始日，默认： 日",
		"optionType": "Number",
		"defaultValue": "0"
	},
	"w-calendar/pickerRender": {
		"description": "自定义日期单元格的内容和样式",
		"optionType": "Function",
		"defaultValue": "-"
	},
	"w-calendar/change": {
		"description": "输入时触发的方法，返回 {当前月份,当前年代区间,当前年的区间,当前年,面板数据集,面板类型,面板的值}({nowMonth,nowRangeAge,nowRangeYear,nowYear,tableOptions,tableType,value})",
		"optionType": "Function",
		"defaultValue": "({nowMonth,nowRangeAge,nowRangeYear,nowYear,tableOptions,tableType,value}) => {}"
	}
}