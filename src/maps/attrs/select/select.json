{
	"w-select/value": {
		"description": "绑定值",
		"optionType": "String | Number | Array",
		"defaultValue": "-"
	},
	"w-select/options": {
		"description": "指定可选项， 每项具体设置参照 fieldNames 参数",
		"optionType": "Array",
		"defaultValue": "-"
	},
	"w-select/fieldNames": {
		"description": "自定义属性的字段名",
		"optionType": "Object",
		"defaultValue": "{ value: 'value', label: 'label', key: 'key', loading: 'loading', disabled: 'disabled' }"
	},
	"w-select/search": {
		"description": "搜索的开关",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-select/disabled": {
		"description": "设置禁用状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-select/loading": {
		"description": "设置载入状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-select/transfer": {
		"description": "是否转移到 body 下",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-select/emptyText": {
		"description": "搜索未匹配到的文案",
		"optionType": "String",
		"defaultValue": "未匹配到结果"
	},
	"w-select/mode": {
		"description": "选择器的模式",
    "options": [
      "single",
      "multiple",
      "tag"
    ],
		"optionType": "String",
		"defaultValue": "single"
	},
	"w-select/size": {
		"description": "设置大小",
    "options": [
      "small",
      "large",
      "不设置"
    ],
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-select/placeholder": {
		"description": "输入框占位文本",
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-select/prefix": {
		"description": "选中的前缀",
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-select/prefix": {
		"description": "选中的后缀",
		"optionType": "String",
		"defaultValue": "-"
	},
	"w-select/placement": {
		"description": "下拉框位置",
    "options": [
      "top",
      "bottom",
      "topLeft",
      "topRight",
      "bottomLeft",
      "bottomRight",
      "left",
      "right",
      "leftTop",
      "leftBottom",
      "rightTop",
      "rightBottom"
    ],
		"optionType": "String",
		"defaultValue": "bottom"
	},
	"w-select/interval": {
		"description": "下拉框间距，单位 p",
		"optionType": "Number",
		"defaultValue": "0"
	},
	"w-select/getContainer": {
		"description": "浮层渲染父节点，默认渲染到 body 上。支持返回 Promise",
		"optionType": "Function",
		"defaultValue": "-"
	},
	"w-select/before": {
		"description": "改变之前触发，返回 Promise 对象",
		"optionType": "Function",
		"defaultValue": "function() {return new Promise((resolve, reject) => {resolve();});}"
	},
	"w-select/change": {
		"description": "改变触发，返回 {Event对象,选中的值,当前数据,所有数据}({ev,value,item,options})",
		"optionType": "Function",
		"defaultValue": "({ev,value,item,options}) => {}"
	},
	"w-select/focus": {
		"description": "获取焦点触发，返回 Event 对象 ( ev )",
		"optionType": "Function",
		"defaultValue": "(ev) => {}"
	},
	"w-select/blur": {
		"description": "失去焦点触发，返回 Event 对象 ( ev )",
		"optionType": "Function",
		"defaultValue": "(ev) => {}"
	},
	"w-select/click": {
		"description": "点击选择框触发，返回 Event 对象 ( ev )",
		"optionType": "Function",
		"defaultValue": "(ev) => {}"
	}
}