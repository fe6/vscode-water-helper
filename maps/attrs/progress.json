{
	"w-progress/strokeWidth": {
		"description": "进度条的宽度，单位 px",
		"optionType": "Number",
		"defaultValue": "8"
	},
	"w-progress/width": {
		"description": "环形进度条画布宽度（只在 type 为 circle 或 dashboard 时可用）",
		"optionType": "Number",
		"defaultValue": "126"
	},
	"w-progress/type": {
		"description": "进度条类型",
    "options": [
      "line",
      "circle",
      "dashboard"
    ],
		"optionType": "String",
		"defaultValue": "line"
	},
	"w-progress/strokeLinecap": {
		"description": "进度条末端的类型",
    "options": [
      "round",
      "square"
    ],
		"optionType": "String",
		"defaultValue": "round"
	},
	"w-progress/status": {
		"description": "进度条当前状态",
    "options": [
      "warning",
      "error",
      "success",
      "不设置"
    ],
		"optionType": "String",
		"defaultValue": "round"
	},
	"w-progress/color": {
		"description": "进度条背景色（会覆盖 status 状态颜色）",
		"optionType": "String | Function(percent) | Array",
		"defaultValue": "-"
	},
	"w-progress/format": {
		"description": "内容的模板函数",
		"optionType": "Function(percent)",
		"defaultValue": "percent => percent + '%'"
	},
	"w-progress/showInfo": {
		"description": "是否显示进度数值或状态图标",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-progress/inside": {
		"description": "进度条显示文字内置在进度条内（只在 type=line 时可用）",
		"optionType": "Boolean",
		"defaultValue": "true"
	}
}